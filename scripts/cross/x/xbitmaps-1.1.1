#!/bin/bash

PKG_NAME=xbitmaps-1.1.1
PKG_URL=http://xorg.freedesktop.org/archive/individual/data/${PKG_NAME}.tar.bz2

pkg_configure() {
	${PKG_SRC_DIR}/configure	--prefix=/usr \
					--build=${CROSSPI_BUILD} \
					--host=${CROSSPI_TARGET} \
					--with-sysroot=${CROSSPI_TARGET_DIR}
}

pkg_install() {
	make install
}
