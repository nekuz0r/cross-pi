#!/bin/bash

PKG_NAME=nettle-2.7.1
PKG_URL=http://ftp.gnu.org/gnu/nettle/${PKG_NAME}.tar.gz

# Depends on openssl & gmp

pkg_configure() {
	${PKG_SRC_DIR}/configure	--prefix=/usr \
					--build=${CROSSPI_BUILD} \
					--host=${CROSSPI_TARGET} \
					--with-sysroot=${CROSSPI_TARGET_DIR} 
}

pkg_build() {
	make -j${CROSSPI_BUILD_NPROC}
}

pkg_install() {
	make install
}
